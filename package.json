{
  "name": "coaching-session-pwas",
  "version": "1.0.0",
  "description": "Coaching Session about Progressive Web Apps",
  "main": "index.js",
  "scripts": {
    "start": "npm run copy | npm run build | npm run serve",
    "copy": "cpx 'src/**/*.{html,js,json,css,svg,eot,woff}' build --watch",
    "build": "rollup -i src/js/main.js -o build/js/bundle.js -f 'iife' --w",
    "serve": "node server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ErikGrijzen/coaching-session-pwas.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ErikGrijzen/coaching-session-pwas/issues"
  },
  "homepage": "https://github.com/ErikGrijzen/coaching-session-pwas#readme",
  "dependencies": {
    "cpx": "^1.5.0",
    "express": "^4.14.0",
    "rollup": "^0.36.1",
    "rollup-watch": "^2.5.0"
  }
}
